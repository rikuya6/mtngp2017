<% @page_title = 'アカウント登録' %>
<header class="v-bottom">
  <div class="container">
    <h1>アカウントの作成</h1>
  </div>
</header>
<div class="col-md-4 col-md-offset-4">
  <%= render 'shared/errors', obj: @user %>
  <%= form_for @user do |f| %>
    <%= render 'form', f: f %>
      <%= f.submit 'アカウントを作成する', class: 'btn btn-primary'%>
  <% end %>
</div>
